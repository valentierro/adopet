{
  "name": "mobile",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "expo": {
    "install": {
      "exclude": [
        "@react-native-community/netinfo",
        "@sentry/react-native",
        "@react-navigation/bottom-tabs",
        "@react-navigation/native",
        "@react-navigation/native-stack"
      ]
    }
  },
  "scripts": {
    "postinstall": "node scripts/patch-expo-router-getDevServer.js",
    "dev": "expo start",
    "dev:clear": "expo start --clear",
    "reset-cache": "node scripts/patch-expo-router-getDevServer.js && rm -rf .expo node_modules/.cache 2>/dev/null; expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build": "expo export",
    "lint": "expo lint",
    "test": "echo \"No tests yet\"",
    "e2e": "maestro test .maestro"
  },
  "dependencies": {
    "@adopet/shared": "workspace:*",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.5.2",
    "@react-navigation/bottom-tabs": "^7.12.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "@sentry/react-native": "^7.12.1",
    "@tanstack/query-async-storage-persister": "^5.59.0",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-persist-client": "^5.59.0",
    "expo": "~54.0.0",
    "expo-clipboard": "~8.0.4",
    "expo-constants": "~18.0.13",
    "expo-device": "^8.0.10",
    "expo-font": "~14.0.11",
    "expo-image": "~3.0.11",
    "expo-file-system": "~19.0.21",
    "expo-image-picker": "^17.0.10",
    "expo-media-library": "~18.2.1",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-notifications": "^0.32.16",
    "expo-router": "~6.0.23",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~19.1.17",
    "typescript": "~5.9.3"
  }
}
